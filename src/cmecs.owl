        <rdfs:label>Sloping (Small Scale Slope)</rdfs:label>
        <cmecs:CMECS_00001668>Original Unit</cmecs:CMECS_00001668>
        <cmecs:unitCode>S02</cmecs:unitCode>
        <cmecs:unitType>CMECS Modifier Value</cmecs:unitType>
    </owl:Class>
    


    <!-- https://w3id.org/CMECS/CMECS_00001576 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00001576">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000809"/>
        <rdfs:comment>No perceptible roughness or texture to sediment surface at scales of less than 1 meter.</rdfs:comment>
