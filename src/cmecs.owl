    <!-- https://w3id.org/CMECS/CMECS_00000715 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000715">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000233"/>
        <rdfs:comment>Depression formed by the abrasive action of sand or sediment driven by the movement of water or ice.</rdfs:comment>
