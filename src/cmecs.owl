        <cmecs:unitType>CMECS Biotic Component: Biotic Community</cmecs:unitType>
    </owl:Class>
    


    <!-- https://w3id.org/CMECS/CMECS_00000132 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000132">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000559"/>
        <rdfs:label>Caprellid Bed</rdfs:label>
        <cmecs:CMECS_00001668>Original Unit</cmecs:CMECS_00001668>
        <cmecs:componentCode>B</cmecs:componentCode>
        <cmecs:unitCode>2.2.2.16.1</cmecs:unitCode>
        <cmecs:unitType>CMECS Biotic Component: Biotic Community</cmecs:unitType>
    </owl:Class>
    


    <!-- https://w3id.org/CMECS/CMECS_00000133 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000133">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000558"/>
        <rdfs:label>Caprellid Communities</rdfs:label>
        <cmecs:CMECS_00001668>Original Unit</cmecs:CMECS_00001668>
        <cmecs:componentCode>B</cmecs:componentCode>
