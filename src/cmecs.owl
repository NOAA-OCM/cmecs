    <!-- https://w3id.org/CMECS/CMECS_00000294 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000294">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000899"/>
        <rdfs:comment>A wave that is trapped by and moves along the Equatorial water mass, which forms a density barrier.</rdfs:comment>
        <rdfs:label>Equatorial Wave</rdfs:label>
        <cmecs:CMECS_00001668>Original Unit</cmecs:CMECS_00001668>
