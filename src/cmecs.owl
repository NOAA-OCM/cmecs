    </owl:Class>
    


    <!-- https://w3id.org/CMECS/CMECS_00000449 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000449">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000722"/>
        <rdfs:comment>A submerged elevation of rounded shape that rises from the ocean floor, but is less prominent than a seamount.</rdfs:comment>
        <rdfs:label>Knoll Seamount</rdfs:label>
        <cmecs:CMECS_00001668>Original Unit</cmecs:CMECS_00001668>
        <cmecs:componentCode>G</cmecs:componentCode>
