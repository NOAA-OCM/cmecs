    <!-- https://w3id.org/CMECS/CMECS_00000696 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000696">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000015"/>
        <rdfs:comment>Usually enclosed areas just landward of the shoreline with a permanent-to-intermittent flooding regime of saline-to-hypersaline waters.</rdfs:comment>
        <rdfs:label>Salt Pond</rdfs:label>
        <cmecs:CMECS_00001668>Original Unit</cmecs:CMECS_00001668>
        <cmecs:componentCode>G</cmecs:componentCode>
