        <cmecs:componentCode>B</cmecs:componentCode>
        <cmecs:unitCode>2.2.1.12.5</cmecs:unitCode>
        <cmecs:unitType>CMECS Biotic Component: Biotic Community</cmecs:unitType>
    </owl:Class>
    


    <!-- https://w3id.org/CMECS/CMECS_00000442 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000442">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000388"/>
        <rdfs:comment>An area of land completely surrounded by water- or an elevated area of land surrounded by swamp or marsh, which is isolated at high water or during floods.</rdfs:comment>
