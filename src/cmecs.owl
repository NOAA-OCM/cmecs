        <cmecs:unitType>CMECS Geoform Subcomponent: Geologic Geoform (Level 1 or Level 2)</cmecs:unitType>
    </owl:Class>
    


    <!-- https://w3id.org/CMECS/CMECS_00000436 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000436">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000824"/>
