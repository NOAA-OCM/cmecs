        <rdfs:label>Seaward Energry Direction</rdfs:label>
        <cmecs:CMECS_00001668>Original Unit</cmecs:CMECS_00001668>
        <cmecs:unitCode>ED06</cmecs:unitCode>
        <cmecs:unitType>CMECS Modifier Value</cmecs:unitType>
    </owl:Class>
    


    <!-- https://w3id.org/CMECS/CMECS_00001568 -->

    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00001568">
