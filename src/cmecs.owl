    <owl:Class rdf:about="https://w3id.org/CMECS/CMECS_00000431">
        <rdfs:subClassOf rdf:resource="https://w3id.org/CMECS/CMECS_00000429"/>
        <rdfs:comment>Large floating ice chunk less than 10 kilometers on its longest axis.</rdfs:comment>
        <rdfs:label>Ice Floe</rdfs:label>
        <cmecs:CMECS_00001668>Original Unit</cmecs:CMECS_00001668>
        <cmecs:componentCode>W</cmecs:componentCode>
